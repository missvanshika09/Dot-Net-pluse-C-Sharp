namespace Day8Delegate
{
    delegate int CADeligiate(int a, int b);
    internal class CADelig
    {
        static int add(int a, int b)
            {
            return a + b;
        }
        static int prod(int a, int b)
        {
            return a * b;
        }

        static void Main(string[] args)
        {
           
